{
	"name": "react-flux-boilerplate",
	"version": "0.0.0",
	"description": "react-flux-boilerplate",
	"main": "js/app.js",
	"dependencies": {
		"flux": "^2.0.1",
		"react": "^0.12.0"
	},
	"devDependencies": {
		"browserify": "^6.2.0",
		"envify": "^3.0.0",
		"jest-cli": "^0.4.3",
		"reactify": "^0.15.2",
		"uglify-js": "~2.4.15",
		"watchify": "^2.1.1"
	},
	"scripts": {
		"start": "watchify -o js/bundle.js -v -d js/app.js",
		"build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.min.js",
		"test": "jest"
	},
	"author": "Jules",
	"browserify": {
		"transform": [
			"reactify",
			"envify"
		]
	},
	"jest": {
		"rootDir": "./js"
	}
}